{
  "name": "capability-jane",
  "version": "1.0.0",
  "license": "MIT",
  "description": "The capability-jane theme",
  "author": "Surgery",
  "browserslist": [
    ">0.5%",
    "not op_mini all",
    "not android <= 4.4.4"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "autoprefixer": "^9.4.4",
    "cssnano": "^4.1.10",
    "custom-event-polyfill": "^1.0.6",
    "element-closest": "^3.0.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "postcss-cli": "^6.1.0",
    "promise-polyfill": "^8.1.0",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.2",
    "smoothscroll-polyfill": "^0.4.2"
  },
  "scripts": {
    "scss": "node-sass --output-style expanded --include-path node_modules ./styles/style.scss ./styles/style.css  --source-map ./styles/style.css.map",
    "postcss": "postcss --u autoprefixer -u cssnano -o ./styles/style.css < ./styles/style.css",
    "build:css": "npm run scss --silent && npm run postcss --silent",
    "build:js": "rollup --config",
    "build": "npm run build:css --silent && npm run build:js --silent",
    "watch:css": "onchange './styles/**/*.scss' -- npm run build:css --silent",
    "watch:js": "rollup --config --watch",
    "watch": "npm-run-all --parallel 'watch:css --silent' 'watch:js --silent'",
    "start": "npm run build --silent && npm run watch --silent"
  },
  "dependencies": {
    "foundation-sites": "^6.5.3",
    "slick-carousel": "^1.8.1"
  }
}
